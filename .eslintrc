{
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "error",
        "quotes": ["error", "double"],
        "eqeqeq": "error",
        "no-use-before-define": "error",
        "no-var": "error",
        "no-redeclare": "error"
    }
}
